{
  "manifest_version": 3,
  "name": "BandLink AI Training Node",
  "version": "1.0.0",
  "description": "Contribute your CPU power for AI training tasks",
  "permissions": [
    "storage",
    "system.cpu",
    "system.memory",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}